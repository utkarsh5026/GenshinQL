{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\" \"scripts/**/*.{ts,js}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\" \"scripts/**/*.{ts,js}\"",
    "scrape": "npx tsx scripts/index.ts",
    "scrape:all": "npx tsx scripts/index.ts all",
    "scrape:characters": "npx tsx scripts/scrape/characters.ts --detailed",
    "scrape:gallery": "npx tsx scripts/scrape/gallery.ts --merge",
    "scrape:talents": "npx tsx scripts/scrape/talents.ts",
    "scrape:weapons": "npx tsx scripts/scrape/weapons.ts --detailed",
    "check-characters": "npx tsx scripts/scrape/characters.ts --check",
    "check-gallery": "npx tsx scripts/scrape/gallery.ts --check",
    "consolidate": "npx tsx scripts/consolidate.ts",
    "generate-primitives": "npx tsx scripts/scrape/primitives.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.978.0",
    "@dnd-kit/core": "^6.1.0",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-hover-card": "^1.1.2",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-tooltip": "^1.1.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "dotenv": "^17.2.3",
    "framer-motion": "^11.11.11",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.453.0",
    "react": "^18.3.1",
    "react-compiler-runtime": "^1.0.0",
    "react-confetti": "^6.1.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.13.0",
    "tailwind-merge": "^2.5.4",
    "tailwind-scrollbar-hide": "^1.1.7",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/node": "^22.7.8",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@types/selenium-webdriver": "^4.1.26",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.20",
    "babel-plugin-react-compiler": "^1.0.0",
    "chalk": "^5.6.2",
    "chromedriver": "^144.0.1",
    "eslint": "^9.11.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "postcss": "^8.4.47",
    "prettier": "3.3.3",
    "selenium-webdriver": "^4.25.0",
    "tailwindcss": "^3.4.14",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8",
    "zod": "^3.23.8"
  }
}
