{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\" \"scripts/**/*.{ts,js}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\" \"scripts/**/*.{ts,js}\"",
    "prepare": "cd .. && npx husky || true",
    "lint-staged": "lint-staged",
    "ci:check": "bash scripts/ci-check.sh",
    "validate": "npm run lint && npm run format:check && tsc --noEmit",
    "build:analyze": "vite build && start dist/stats.html",
    "scrape": "npx tsx scripts/index.ts",
    "scrape:all": "npx tsx scripts/index.ts all",
    "scrape:characters": "npx tsx scripts/scrape/characters.ts --detailed",
    "scrape:gallery": "npx tsx scripts/scrape/gallery.ts --merge",
    "scrape:talents": "npx tsx scripts/scrape/talents.ts",
    "scrape:weapons": "npx tsx scripts/scrape/weapons.ts --detailed",
    "check-characters": "npx tsx scripts/scrape/characters.ts --check",
    "check-gallery": "npx tsx scripts/scrape/gallery.ts --check",
    "consolidate": "npx tsx scripts/consolidate.ts",
    "generate-primitives": "npx tsx scripts/scrape/primitives.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.985.0",
    "@dnd-kit/core": "^6.1.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dotenv": "^17.2.4",
    "framer-motion": "^12.34.0",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.563.0",
    "react": "^18.3.1",
    "react-compiler-runtime": "^1.0.0",
    "react-confetti": "^6.4.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.13.0",
    "tailwind-merge": "^3.4.0",
    "tailwind-scrollbar-hide": "^4.0.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.0",
    "@commitlint/config-conventional": "^20.4.0",
    "@eslint/js": "^9.39.2",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.1.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@types/selenium-webdriver": "^4.1.26",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.24",
    "babel-plugin-react-compiler": "^1.0.0",
    "chalk": "^5.6.2",
    "chromedriver": "^145.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^17.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "3.8.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "selenium-webdriver": "^4.25.0",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "zod": "^4.3.6"
  }
}
